{
  "extends": "../../tslint.json",
  "no-empty": false,
  "no-non-null-assertion": true,
  "no-redundant-jsdoc": false,
  "no-switch-case-fall-through": true,
  "no-var-requires": false,
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ]
  }
}
